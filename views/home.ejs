<%- include("partials/header"); -%>

<h1>Home Page</h1>
<p><%= startingContent %></p>

<% newEntry.forEach(function(post){ %>

<h2><%= post.title %></h2>
<p>
  <%=post.content.substring(0, 100) + "..."%>
  <a href="/posts/<%= post._id %>">Read More</a>
</p>

<%}) %>

<%- include("partials/footer"); -%>
